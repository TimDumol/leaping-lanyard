#home-page.row(ng-controller='HomeAnonCtrl', ng-if='!CurrentUser.loggedIn()')
  .span12.row
    img(src='/img/banner.png', alt='Need a hand with your errands? Have your friendly neighbours help you out!')#banner
  img(src='/img/popularerrands.png', alt='Popular Errands')
  - var names = ['laundry', 'delivery', 'grocery']
  ul#teasers.row.span12
    - each name in names
      li
        img(src='/img/#{name}.png')
